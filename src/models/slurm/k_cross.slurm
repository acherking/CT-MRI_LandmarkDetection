#!/bin/bash
#SBATCH --nodes 1
#SBATCH -p gpu-a100
#SBATCH --gres=gpu:1
#SBATCH --time 100:00:00
#SBATCH --mem=200G
#SBATCH --output ./output/slurm-%j.out

module load CUDA/11.7.0
module load TensorFlow/2.11.0-CUDA-11.7.0

k=20
model_tag=cropped
for i in $(eval echo "{1..$k}")
do
   echo "K=$k, [$i] times"
   python3 /data/gpfs/projects/punim1836/Training/cross_validation.py $k $i $model_tag
done

